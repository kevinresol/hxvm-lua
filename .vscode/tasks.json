{
    "version": "2.0.0",
    "command": "travix",
    "args": ["node", "-lib", "hxjs-fengari"],
    "problemMatcher": {
        "owner": "haxe",
        "pattern": {
            "regexp": "^(.+):(\\d+): (?:lines \\d+-(\\d+)|character(?:s (\\d+)-| )(\\d+)) : (?:(Warning) : )?(.*)$",
            "file": 1,
            "line": 2,
            "endLine": 3,
            "column": 4,
            "endColumn": 5,
            "severity": 6,
            "message": 7
        }
	},
	"group": {
		"kind": "build",
		"isDefault": true
	}
}
